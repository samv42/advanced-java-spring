<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="beatlesRecord" class="platform.codingnomads.co.corespring.lab.complete.Record">
        <property name="artist" value="The Beatles"/>
        <property name="album" value="Abbey Road"/>
    </bean>

    <bean id="bowieRecord" class="platform.codingnomads.co.corespring.lab.complete.Record">
        <property name="artist" value="David Bowie"/>
        <property name="album" value="Hunky Dorey"/>
    </bean>
</beans>

